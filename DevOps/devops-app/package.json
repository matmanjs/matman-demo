{
    "name": "devops-app",
    "scripts": {
        "test": "cross-env DEBUG=nightmare node debug/test.js",
        "test:unit": "cross-env DWT_MODE=unit npm test",
        "test:e2e": "cross-env DWT_MODE=e2e npm test",
        "test:e2e:direct": "cross-env DWT_MODE=e2e npm test",
        "bootstrap:prod": "cross-env MOCKSTAR_PORT=9527 WHISTLE_PORT=8899 node debug/bootstrap.js",
        "bootstrap": "cross-env-shell MOCKSTAR_PORT=9527 WHISTLE_PORT=8899 \"npm run e2e-bootstrap && npm run test:e2e:direct && npm run e2e-clean\""
    },
    "dependencies": {
        "devops-web-test": "^1.0.1",
        "lodash": "^4.17.15"
    },
    "devDependencies": {
        "cross-env": "^7.0.2"
    }
}
