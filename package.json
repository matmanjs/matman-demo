{
    "name": "matman-demo",
    "private": true,
    "scripts": {
        "start": "react-app-rewired start",
        "build": "react-app-rewired build",
        "use-whistle-dev": "cd DevOps/whistle && npm run start-dev",
        "use-whistle-prod": "cd DevOps/whistle && npm run start-prod",
        "test": "cd DevOps/devops-app && npm install && npm run test",
